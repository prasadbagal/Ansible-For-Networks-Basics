---


- hosts: ios
  gather_facts: no
  connection: local


  vars:
    go_ahead_command: true
    go_ahead_priting: true


  tasks:
  - name: show commands
    ios_command:
      commands:
        - show ip interf b
    register: cmd_op
    when: go_ahead_command == true


  - name: print it
    debug:
      var: cmd_op
    when: go_ahead_priting == true
